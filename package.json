{
  "name": "solid-js",
  "description": "A declarative JavaScript library for building user interfaces.",
  "version": "0.12.0",
  "author": "Ryan Carniato",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/ryansolid/solid"
  },
  "main": "lib/index.js",
  "module": "dist/index.js",
  "types": "types/index.d.ts",
  "sideEffects": false,
  "scripts": {
    "prebuild": "rimraf dist/* lib/* types/* && dom-expressions",
    "build": "run-p -cnl build:*",
    "build:js": "rollup -c",
    "build:types": "tsc && ncp ./src/dom/runtime.d.ts ./types/dom/runtime.d.ts",
    "bench": "node --allow-natives-syntax bench/bench.js",
    "prepublishOnly": "npm run build",
    "test": "npm run build && jest --coverage",
    "test:types": "tsc --project tsconfig.test.json"
  },
  "devDependencies": {
    "@babel/core": "7.6.4",
    "@babel/preset-env": "7.6.3",
    "@babel/preset-typescript": "^7.6.0",
    "babel-plugin-jsx-dom-expressions": "~0.13.0",
    "coveralls": "^3.0.6",
    "ncp": "2.0.0",
    "dom-expressions": "0.13.0",
    "hyper-dom-expressions": "~0.13.0",
    "jest": "~24.9.0",
    "lit-dom-expressions": "~0.13.0",
    "npm-run-all": "^4.1.5",
    "rimraf": "^2.6.3",
    "rollup": "^1.24.0",
    "rollup-plugin-babel": "^4.3.3",
    "rollup-plugin-node-resolve": "^5.2.0",
    "typescript": "^3.6.4"
  }
}
